{
  "name": "comp426-health-app",
  "version": "1.0.0",
  "description": "RESTful Web App: Class Project",
  "main": "index.js",
  "scripts": {
    "test:backend": "npm run server-build && mocha ./backend/api/dist/tests/**/*.test.js --reporter spec --exit --use_strict",
    "start": "node ./dist/server.js",
    "mocha": "mocha --requires js:babel-core/register --reporter spec --exit --use_strict ",
    "build": "babel src/ --out-dir dist/ ",
    "dev": "nodemon --watch src --exec 'npm run build && npm start'",
    "app-build": "babel ./frontend/src/ --out-dir ./frontend/dist/",
    "server-build": "rimraf ./backend/api/dist/ && babel ./backend/api/src/ --out-dir ./backend/api/dist/",
    "server-start": "node ./backend/api/dist/server.js",
    "server": "nodemon --watch ./backend/api/src/ --exec 'npm run server-build && npm run server-start'",
    "mock-server-build": "babel ./backend/mock/src/ --out-dir ./backend/mock/dist/",
    "mock-server-start": "node ./backend/mock/dist/server.js",
    "mock-server": "nodemon --watch ./backend/mock/src/ --exec 'npm run mock-server-build && npm run mock-server-start'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmr1027/comp426-health-app.git"
  },
  "author": "Austin Tinsley, Eric Xin, Grace Abellanosa, Preston Robinson",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/pmr1027/comp426-health-app/issues"
  },
  "homepage": "https://github.com/pmr1027/comp426-health-app#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "convert-units": "^2.3.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.1",
    "osprey": "^0.4.1",
    "sequelize": "^4.27.0",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-minify": "^0.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.10.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-chai-friendly": "^0.4.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.58.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "webpack": "^3.9.1",
    "winston": "^2.4.0"
  }
}
